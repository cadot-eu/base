<table class="table table-striped table-bordered {% if block.data.stretched %}w-100{% endif %}">
	<thead>
		{% if block.data.withHeadings %}
			<tr>
				{% for heading in block.data.content[0] %}
					<th>{{ heading }}</th>
				{% endfor %}
			</tr>
		{% endif %}
	</thead>
	<tbody>
		{% for row in block.data.content %}
			{% if not loop.first or not block.data.withHeadings %}
				<tr>
					{% for cell in row %}
						<td>{{ cell }}</td>
					{% endfor %}
				</tr>
			{% endif %}
		{% endfor %}
	</tbody>
</table>
