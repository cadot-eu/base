{
    "name": "votre-vendor/base-project-template",
    "description": "Template de projet Symfony personnalisable pour dÃ©marrage rapide.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "cadot.eu",
            "email": "base@cadot.eu"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/symfony": "^6.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r 'file_exists(\".env\") || copy(\".env.example\", \".env\");'"
        ],
        "post-create-project-cmd": [
            "@php bin/console cache:clear || true"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
